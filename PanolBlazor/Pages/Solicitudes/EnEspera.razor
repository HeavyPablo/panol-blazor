@page "/solicitudes/en_espera"
@attribute [Authorize]
@layout Layout.SolicitudesLayout
@using System.Net.Http
@inject HttpClient Http

<SolicitudesTableGeneric Solicitudes="Solicitudes" Titulo="En Espera"></SolicitudesTableGeneric>

@code {
    private List<PanolBlazor.DataModel.SolicitudModel> Solicitudes;

    protected override async Task OnInitializedAsync()
    {
        await Task.Delay(2000);
        Solicitudes = await Http.GetJsonAsync<List<PanolBlazor.DataModel.SolicitudModel>>("http://localhost:8080/solicitud");
    }
}
